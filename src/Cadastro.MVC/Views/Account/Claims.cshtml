
@{
    ViewData["Title"] = "Claims";
}

<h1>Claims</h1>

<table class="table table-striped" aria-describedby="Tabela com as claims">
    <thead>
        <tr>
            <th>
                Claim
            </th>
            <th>
                Value
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var claim in User.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </tbody>
</table>

